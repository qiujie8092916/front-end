@mixin animate($attrName, $customName, $duration, $timingFunction: ease){
	-webkit-#{$attrName}: $customName $duration;
	-moz-#{$attrName}: $customName $duration;
	-ms-#{$attrName}: $customName $duration;
	-o-#{$attrName}: $customName $duration;
	#{$attrName}: $customName $duration;
}

@mixin transform($keyname){
	-webkit-transform: $keyname;
	-moz-transform: $keyname;
	-ms-transform: $keyname;
	-o-transform: $keyname;
	transform: $keyname;
}

@mixin border-radius($value){
	-webkit-border-radius: $value;
	-moz-border-radius: $value;
	-ms-border-radius: $value;
	-o-border-radius: $value;
	border-radius: $value;
}

@mixin box-shadow($h, $v, $spread, $color){
	-webkit-box-shadow: $h $v $spread $color;
	-moz-box-shadow: $h $v $spread $color;
	-ms-box-shadow: $h $v $spread $color;
	-o-box-shadow: $h $v $spread $color;
	box-shadow: $h $v $spread $color;
}

@mixin box-sizing($value){
	-webkit-box-sizing:  $value;
	-moz-box-sizing:  $value;
	-ms-box-sizing:  $value;
	-o-box-sizing:  $value;
	box-sizing:  $value;
}

/***************global*****************/
*{
	padding:0;
	margin:0	
}

body{
	background-color: #f1f2f3;
  font-size: 12px;
  line-height: 1.5;
  font-family: tahoma,arial,sans-serif;
  color: #666;
}

div, a, input{
  @include box-sizing(border-box);	
}

.col-md{
	width: 1200px;
	margin: 0 auto;		
}

.inline{
	display: inline-block;
}

.arrow{
	background: url(../../imgs/arrow.png) no-repeat;
	overflow: hidden;
  display: inline-block;
  width: 10px;
  height: 10px;
  margin-left: 4px;
  @include animate(transition, transform, .2s, ease-out);
}

.dt-vline{
	float: right;
	width: 1px;
	height: 24px;
	margin-top: 20px;
	background-color: #ebebeb;
}

.hidden{
	display: none;	
}

.dt-menu{
  border: 1px solid rgba(0,0,0,.12);
  border-top: 1px solid #ddd;
  @include box-shadow(0, 1px, 3px, rgba(0,0,0,.096));	
}

.gea{
	text-decoration: none;
	color: #444;
	font-size: 13px;
	
	&:hover{
		text-decoration: underline;
	}
}


